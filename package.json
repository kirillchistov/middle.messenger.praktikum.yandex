{
  "name": "middle.messenger.praktikum.yandex",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy:branch": "npm run build && node ./scripts/deploy-to-branch.cjs",
    "typecheck": "tsc -b",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:all": "npm run lint && npm run lint:style && npm run typecheck && npm test",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite build && vite preview --port 3000",
    "format": "prettier --write .",
    "test": "vitest",
    "test:vitest": "vitest",
    "test:jest": "jest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "handlebars": "^4.7.8",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "7",
    "@typescript-eslint/parser": "7",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.16",
    "eslint": "~8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.32",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-handlebars": "^0.0.1",
    "sinon": "^21.0.1",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-handlebars": "^2.0.0",
    "vitest": "^4.0.18"
  }
}
